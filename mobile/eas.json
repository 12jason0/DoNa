{
    "cli": {
        "version": ">= 16.28.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": false
            },
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "autoIncrement": true,
            "distribution": "store",
            "env": {
        "EXPO_PUBLIC_REVENUECAT_API_KEY_IOS": "@REVENUECAT_API_KEY_IOS",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID": "@REVENUECAT_API_KEY_ANDROID"
      },
            "android": {
                "buildType": "app-bundle"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
